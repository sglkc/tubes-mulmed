{
  "applicationId": "js.neutralinojs.neu",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/src/dist/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "filesystem.readDirectory"
  ],
  "modes": {
    "window": {
      "title": "Hello Neutralinojs",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/src/dist/icon.png"
    }
  },
  "cli": {
    "binaryName": "neu",
    "resourcesPath": "/src/dist/",
    "extensionsPath": "/extensions/",
    "binaryVersion": "5.5.0",
    "frontendLibrary": {
      "patchFile": "/src/index.html",
      "devUrl": "http://localhost:5173",
      "projectPath": "/src",
      "initCommand": "pnpm i",
      "devCommand": "pnpm dev",
      "buildCommand": "pnpm build"
    }
  }
}
